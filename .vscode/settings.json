{
    "dart.flutterSdkPaths": [],
    "dart.enableSdkFormatter": true,
    "dart.lineLength": 80,
    "dart.insertArgumentPlaceholders": false,
    "dart.previewFlutterUiGuides": true,
    "dart.previewFlutterUiGuidesCustomTracking": true,
    "dart.hotReloadOnSave": "all",    "dart.devToolsTheme": "dark",    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true
    },
    "github.copilot.terminal.suggestCommands": "auto",
    "github.copilot.chat.executeImmediately": "auto",
    "github.copilot.chat.followUp": "always",
    "github.copilot.chat.welcomeMessage": "never",
    "github.copilot.chat.localeOverride": "auto",
    "github.copilot.conversation.enableAutoSend": true,
    "github.copilot.advanced": {
        "inlineSuggestEnable": true,
        "authProvider": "github",
        "debug.overrideEngine": "copilot-chat",
        "debug.useNodeFetcher": true
    },

    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.preview": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    }
}
